    "@dimerapp/content": "^5.0.0",
    "@dimerapp/docs-theme": "^4.0.4",
    "@dimerapp/edge": "^5.0.0",
    "@docsearch/css": "^3.5.2",
    "@docsearch/js": "^3.5.2",
    "@swc/core": "^1.4.1",
    "@types/node": "^20.11.17",
    "alpinejs": "^3.13.5",
    "collect.js": "^4.36.1",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "edge-uikit": "^1.0.0-1",
    "medium-zoom": "^1.1.0",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "^0.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "undici": "^6.6.2",
    "unpoly": "^3.7.3",
    "vite": "^5.1.2"
